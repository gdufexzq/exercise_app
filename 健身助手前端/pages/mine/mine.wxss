.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  background-color: white;
  height: 100vh;
} 

.head_image image{
  width: 120rpx;
  height: 120rpx;
  display: flex;
  margin: 30rpx auto;
  border-radius: 60rpx;
}

.head_image text {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: black;
}

.info{
  display: flex;
  flex-direction: raw;
  margin: 7vh 0rpx;
  margin-top: 5vh;
}

.info view {
  margin: 0rpx 30rpx;
}

.info view text {
  display: flex;
  font-size: 30rpx;
  margin: 15rpx auto;
}

.info>view>text:first-child{
  display: flex;
  flex-direction: column;
  align-items: center;
  color: rgb(8, 195, 241);
  font-weight: bold;
}

.text_center {
  margin: 42rpx 10rpx;
}

/* button::after {
  border: none;
} */

.nav {
  width: 100%;
  border-top: 15rpx #efefef solid;
}

.nav image {
  float: left;
  width: 50rpx;
  height: 50rpx;
  margin: 20rpx 40rpx;
}

.nav>view {
  width: 100%;
  height: 90rpx;
  border-bottom: 1rpx #efefef solid;
}

.text {
  font-size: 35rpx;
  padding-top: 23rpx;
}

.skip {
  float: right;
  margin-right: 40rpx;
}

