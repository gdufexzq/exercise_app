.mydata_container {
  width: 100%;
  height: 100vh;
  background-color: #584F60;
}

.mydata {
  text-align: center;
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  font-size: 1.4rem;
  font-weight: bold;
  color: #24c68a;
}

.totalCoin {
  width: 100%;
  height: 200rpx;
  margin-top: 30rpx;
  background-color: #24c68a;
}

.totalCoin_text {
  margin-left: 20rpx;
  padding: 10rpx;
  color: #bfc2c7;
  font-weight: bold;
}

.totalCoin_context {
  margin-left: 50rpx;
  margin-top: 30rpx;
  font-size: 1.6rem;
  font-weight: bold;
  text-align: center;
  color: #fff;
}
.totalCount {
  width: 100%;
  height: 200rpx;
  margin-top: 30rpx;
  background-color: #7695ce;
}

.totalCount_text {
  margin-left: 20rpx;
  padding: 10rpx;
  color: #bfc2c7;
  font-weight: bold;
}



.totalCount_context {
  margin-left: 50rpx;
  margin-top: 30rpx;
  font-size: 1.6rem;
  font-weight: bold;
  text-align: center;
  color: #fff;
}

.skip {
  float: right;
  color: #584F60;
}